import java.util.Scanner;

/**
 * Клас для обчислення чисел Фібоначчі.
 *
 * @author (Ваше ім'я)
 * @version (Версія)
 */
public class Fibonacci {

    /**
     * Метод для обчислення N-го числа Фібоначчі.
     *
     * @param n Ціле число, для якого потрібно обчислити число Фібоначчі.
     * @return N-е число Фібоначчі.
     */
    public static int fibonacci(int n) {
        if (n == 0) {
            return 0;
        } else if (n == 1 || n == -1) {
            return 1;
        } else if (n > 0) {
            int a = 0, b = 1;
            for (int i = 2; i <= n; i++) {
                int temp = a + b;
                a = b;
                b = temp;
            }
            return b;
        } else {
            // Для від'ємних N використовуємо формулу F(-n) = (-1)^(n+1) * F(n)
            int fibmin = fibonacci(-n);
            return (n % 2 == 0) ? -fibmin : fibmin;
        }
    }

    /**
     * Головна функція для запуску програми.
     *
     * @param args Аргументи командного рядка (не використовуються).
     */
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        System.out.print("Введіть N: ");
        int N = scanner.nextInt();

        System.out.println("Число Фібоначчі для N = " + N + " : " + fibonacci(N));
    }
}
